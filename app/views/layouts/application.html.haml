    /[if lt IE 9]
      = javascript_include_tag 'html5shiv.min'

    %link{rel: 'stylesheet', href: 'https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.3.0/styles/solarized-dark.min.css'}
